py("event","viewItem",{
    "product_no":"商品编号",
    "spu_id":"标准化产品单元ID",
    "name":"商品名称",
    "category":"商品分类",
    "price":"商品销售价格",
    "orig_price":"商品显示价格",
    "currency_code":"货币类型，默认为人民币",
    "product_url":"商品链接",
    "pic_width":"图片宽",
    "pic_height":"图片高",
    "pc_pic_url":"商品图链接",
    "promotion":"促销信息，例如：热卖,促销",
    "sold_out":"售空标识",
    "brand":"品牌名称",
    "discount":"折扣"
});
py('send','visit','viewItem');